         if (params.data.isTotalRow) {
           return (
             <span style={{ fontWeight: "bold", color: "#000000" }}>
               {params.value}
             </span>
           );
         }
         const parts = (params.data.type || "").split(" ");
         return <span>{parts[0] || ""}</span>;
